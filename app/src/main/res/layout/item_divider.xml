<?xml version="1.0" encoding="utf-8"?>
<layout>

  <data>
    <variable
      name="item"
      type="com.saniou.santieba.vo.DividerItem"
      />
  </data>

  <View
    xmlns:android="http://schemas.android.com/apk/res/android"
    dividerBackgroundColor="@{item.backgroundColor}"
